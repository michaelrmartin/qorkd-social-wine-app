@posts.map do |post|
{
  id: post.id,
  user_id: post.user_id,
  user_name: post.user.username,
  wine_id: post.wine_id,
  wine_name: post.wine.name,
  description: post.description,
  rating: post.rating,
  post_img_url: post.post_img_url,
  friendly_created_at: post.friendly_created_at,
  friendly_updated_at: post.friendly_updated_at,
}
end